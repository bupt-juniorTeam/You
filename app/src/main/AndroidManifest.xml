<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.you.android">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".YouApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.You">
        <activity android:name=".ui.homepage.HomePageActivity"></activity>
        <activity android:name=".ui.personcenter.PersonCenterActivity"></activity>
        <activity android:name=".ui.roomlist.RoomListActivity" />
        <activity android:name=".ui.login.LoginActivity" />
        <activity android:name=".ui.SplashActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>